    "type": "simplesamlphp-module",
    "keywords": [ "idp", "fido2", "webauthn", "2fa" ],
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Stefan Winter",
            "email": "stefan.winter@restena.lu"
        }
